From: Daniel Kucera <daniel.kucera@gmail.com>
Date: Thu, 4 May 2023 21:43:40 +0200
Subject: [PATCH] add hantek.sys

---
 configure    | 2 ++
 configure.ac | 1 +
 2 files changed, 3 insertions(+)

diff --git a/configure b/configure
index c90e5ac9555..dc78b1a3bfc 100755
--- a/configure
+++ b/configure
@@ -1344,6 +1344,7 @@ enable_glu32
 enable_gphoto2_ds
 enable_gpkcsp
 enable_hal
+enable_hantek_sys
 enable_hhctrl_ocx
 enable_hid
 enable_hidclass_sys
@@ -20504,6 +20505,7 @@ wine_fn_config_makefile dlls/glu32 enable_glu32
 wine_fn_config_makefile dlls/gphoto2.ds enable_gphoto2_ds
 wine_fn_config_makefile dlls/gpkcsp enable_gpkcsp
 wine_fn_config_makefile dlls/hal enable_hal
+wine_fn_config_makefile dlls/hantek.sys enable_hantek_sys
 wine_fn_config_makefile dlls/hhctrl.ocx enable_hhctrl_ocx
 wine_fn_config_makefile dlls/hid enable_hid
 wine_fn_config_makefile dlls/hid/tests enable_tests
diff --git a/configure.ac b/configure.ac
index c8ef570a60d..f67d95e964c 100644
--- a/configure.ac
+++ b/configure.ac
@@ -3313,6 +3313,7 @@ WINE_CONFIG_MAKEFILE(dlls/glu32)
 WINE_CONFIG_MAKEFILE(dlls/gphoto2.ds)
 WINE_CONFIG_MAKEFILE(dlls/gpkcsp)
 WINE_CONFIG_MAKEFILE(dlls/hal)
+WINE_CONFIG_MAKEFILE(dlls/hantek.sys)
 WINE_CONFIG_MAKEFILE(dlls/hhctrl.ocx)
 WINE_CONFIG_MAKEFILE(dlls/hid)
 WINE_CONFIG_MAKEFILE(dlls/hid/tests)
